<%= form_with(scope: :users, url: users_path, local:true, class:"form-signin") do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, class:"form-control" %>

  <%= form.label :email %>
  <%= form.text_field :email, class:"form-control" %>

  <%= form.label :password %>
  <%= form.text_field :password, type:"password", class:"form-control" %>

  <%= form.submit "登録", class:"btn btn-lg btn-primary btn-block" %>
<% end %>
