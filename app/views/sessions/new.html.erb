<%= form_with(scope: :session, url: sessions_path, local:true) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.label :password_digest %>
  <%= form.text_field :password_digest %>

  <%= form.submit %>
<% end %>
<%= link_to 'sing in', new_user_path %>
