<%= form_with(model: @feed, url: feeds_path, local: true) do |form| %>
 
 
 
  <%= form.hidden_field :coment %>
  <%= form.hidden_field :image_cache %>


  <strong><h2>coment:</h2></strong>
  <%= @feed.coment %>
  <strong><h2>Image:</h2></strong>
  <%= image_tag @feed.image.url %>




  <%= form.submit "投稿する" %>
<% end %>

<%= link_to '戻る', images_path  %>



